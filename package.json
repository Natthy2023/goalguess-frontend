{
  "name": "goalguess-arena",
  "version": "1.0.0",
  "description": "Ultimate Football Player Guessing Game",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "node --watch server/index.js",
    "dev:client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "seed": "node server/seeds/seedFromAPI.js",
    "seed:local": "node server/seeds/seedFirebase.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "socket.io": "^4.7.2",
    "express-rate-limit": "^7.0.0",
    "axios": "^1.5.0",
    "firebase-admin": "^12.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.1"
  }
}
